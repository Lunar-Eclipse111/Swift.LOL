local lib = loadstring(game:HttpGet('https://raw.githubusercontent.com/Lunar-Eclipse111/Swift.LOL/main/Swift.UI'))()



getgenv().spinbot = false
getgenv().Spinspeed = 0
getgenv().Autofarm = false
