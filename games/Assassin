-- main ui color
getgenv().UIClr = Color3.fromRGB(110, 118, 250)
getgenv().ESPClr = UIClr
getgenv().FOVClr = UIClr

local main = lib:Make({

	Image = 12759240856
})
-- Aim Section
local aim = main:Tab({
	Text = "Silent Aim"
})

local AimTog = aim:Toggle({
	Text = "Enable",
	func = function(e) 
		getgenv().SilentAim = e
	end
})
AimTog:Bind({
	def = Enum.KeyCode.X
})

local CicleTog = aim:Toggle({
	Text = "Show FOV",
	func = function(e) 
		FOV_Circle.Visible = e
	end
})
CicleTog:Bind({
	def = Enum.KeyCode.C
})

aim:Slider({
	Text = "FOV Size",
	max = 300,
	min = 0,
	func = function(e)
		FOV_Circle.Radius = e
	end
})

aim:ColorPicker({
	Text = "FOV Color",
	Def = getgenv().FOVClr,
	func = function(e)
		FOV_Circle.Color = e
	end
})
-- Aim Section End


-- Visual section start

local vis = main:Tab({
	Text = "ESP/Visual"
})

local vistog = vis:Toggle({
	Text = "Enable Chams",
	func = function(e) 
		getgenv().ESP = e
	end
})

vis:ColorPicker({
	Text = "Chams Color",
	Def = getgenv().ESPClr,
	func = function(e)
		getgenv().ESPClr = e
	end
})

-- END VISUAL TAB

-- START GAME TAB
local Game = main:Tab({
	Text = "Game"
})

Game:Slider({
	Text = "Game FOV",
	max = 100,
	min = 70,
	func = function(e)
		task.spawn(function()
			workspace.CurrentCamera.FieldOfView = e
		   end)
		
	end
})
Game:Slider({
	Text = "Player walkspeed",
	max = 60,
	min = 16,
	func = function(e)
	task.spawn(function()
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = e
	end)

			

	
	end
})

Game:Button({
	Text = "Reset gravity",
	func = function()
		workspace.Gravity = 196.2;
	end
})

local bhoptog = Game:Toggle({
	Text = "Bhop",
	func = function(e) 
		local humanoid = script.Parent:WaitForChild("Humanoid")
game:GetService("RunService").Heartbeat:Connect(function()
	humanoid.Jump = v
end)
	end
})
--END GAME TAB

