local Shadow = game.Players.LocalPlayer.PlayerGui.ScreenGui.UI.nKills.ShadowText
watermarkPlacement.Text = watermark
Shadow.Text = watermark


local lib = loadstring(game:HttpGet('https://raw.githubusercontent.com/Lunar-Eclipse111/Swift.LOL/main/NewUI.Lua'))()


--getgenv().spinbot = false
--getgenv().Spinspeed = 12
getgenv().Autofarm = false
getgenv().infJump = false
getgenv().SilentAim = false
getgenv().ESP = false
getgenv().Target = false
getgenv().WS = 16
getgenv().infJump = false


-- Defult Colors

-- main ui color
getgenv().UIClr = Color3.fromRGB(110, 118, 250)
getgenv().ESPClr = UIClr
getgenv().TargetClr = Color3.fromRGB(255,0,0)
getgenv().FOVClr = UIClr

_G.TargetClr =  Color3.fromRGB(255,0,0)
_G.EspClr = UIClr or Color3.fromRGB(0,0,255)

_G.ESPToggle = false
_G.TargetESP = false-- Add1 - Variable storing if user wants the target ESP enabled, change to false if you don't want it 


local main = lib:Draw({
	game = "Assassin!"	
})
local silent = main:Tab({
	Text = "Silent Aim",
	Left = "Toggles",
	Right = "Configuration"
})


local toggle = silent:Toggle({
	Text = "Enable Silent Aim",
	Side = "L",
	func = function(e)
		getgenv().SilentAim = e
	end
})

local bind = toggle:Bind({
	def = Enum.KeyCode.X
})

local toggle = silent:Toggle({
	Text = "Show FOV",
	Side = "L",
	func = function(e)
		FOV_Circle.Visible = e
	end
})

local bind = toggle:Bind({
	def = Enum.KeyCode.C
})

local slider = silent:Slider({
	Text = "FOV Size",
	Side = "R",
	min = 0,
	max = 250,
	prefix = "%",
	func = function(e)
		FOV_Circle.Radius = e
	end
})

local ColorPicker = silent:ColorPicker({
	Text = "Fov Color",
	Side = "R",
	def = FOVClr,
	func = function(e)
		FOV_Circle.Color = e
	end
})



