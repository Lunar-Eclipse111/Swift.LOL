-- PLAYER TAB
local playa = main:Tab({
	Text = "Player",
	Left = "Toggles",
	Right = "Configuration"
})

local toggle = playa:Toggle({
	Text = "Inf Jump",
	Side = "L",
	func = function(e)
		getgenv().infJump = e
	end
})

local char = game.Players.LocalPlayer.Character

getgenv().FF = false
local toggle = playa:Toggle({
	Text = "Forcfield character",
	Side = "L",
	func = function(e)
		 getgenv().FF = e
		if e == true then
			getgenv().FF = true
			local RunService = game:GetService("RunService")
			char.Head.Material = Enum.Material.ForceField
			char.Torso.Material = Enum.Material.ForceField
			char["Left Leg"].Material = Enum.Material.ForceField
			char["Right Leg"].Material = Enum.Material.ForceField
			char["Left Arm"].Material = Enum.Material.ForceField
			char["Right Arm"].Material = Enum.Material.ForceField
		end
		if e == false then
			getgenv().FF = false
			local plr = game.Players.LocalPlayer
			local char = plr.Character
			local RunService = game:GetService("RunService")
			char.Head.Material = Enum.Material.SmoothPlastic
			char.Torso.Material = Enum.Material.SmoothPlastic
			char["Left Leg"].Material = Enum.Material.SmoothPlastic
			char["Right Leg"].Material = Enum.Material.SmoothPlastic
			char["Left Arm"].Material = Enum.Material.SmoothPlastic
			char["Right Arm"].Material = Enum.Material.SmoothPlastic
		end
	end
})
local head = char.Head.Color
local torso = char.Torso.Color
local legl = char["Left Leg"].Color
local legR = char["Left Leg"].Color
local arml = char["Left Arm"].Color
local armR = char["Right Arm"].Color

local btn = playa:Button({
	Text = "Reset Gravity",
	Side = "L",
	func = function()
		workspace.Gravity = 196.2;
	end
})

local ColorPicker = playa:ColorPicker({
	Text = "ForceField Color",
	Side = "R",
	def = getgenv().ESPClr,
	func = function(e)
		if getgenv().FF == true then
			char.Head.Color = e
			char.Torso.Color = e
			char["Left Leg"].Color = e
			char["Right Leg"].Color = e
			char["Left Arm"].Color = e
			char["Right Arm"].Color = e
			end

			if getgenv().FF == false then
				char.Head.Color = head
				char.Torso.Color = torso
				char["Left Leg"].Color = legl
				char["Right Leg"].Color = legR
				char["Left Arm"].Color = arml
				char["Right Arm"].Color = armR
				end
	end
})

local slider = playa:Slider({
	Text = "Game FOV",
	Side = "R",
	min = 70,
	max = 130,
	prefix = "%",
	func = function(e)
		task.spawn(function()
			workspace.CurrentCamera.FieldOfView = e
		   end)
	end
})

local slider = playa:Slider({
	Text = "Player Walkspeed",
	Side = "R",
	min = 16,
	max = 50,
	prefix = "%",
	func = function(e)
		task.spawn(function()
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = e
		end)
	end
})
