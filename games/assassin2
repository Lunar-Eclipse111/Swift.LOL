local Spinbot = farmi:Toggle({
	Text = "SpinBot",
	func = function(e) 
		getgenv().spinbot = e

        if e == false then
			_G.updateSpeed = _G.updateSpeed
			wait(0.1)
_G.RealSpeed = 0
        end

if e == true then
	_G.RealSpeed = _G.updateSpeed
end

	end
})
