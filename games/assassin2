a:AdjustSpeed(4) 
-- Crawl ANIMATION END

--lay ANIMATION START
local b1 = "181526230"
local b2 = Instance.new("Animation")
b2.AnimationId = "rbxassetid://"..b1
local b = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(b2)
b:AdjustSpeed(4) 
-- lay ANIMATION END





local farm = an:Toggle({
	Text = "Crawl animation",
	func = function(e) 
	if e == true then
a:Play()
	else
		a:Stop()
	end
	end
})

local farm = an:Toggle({
	Text = "Lay animation",
	func = function(e) 
	if e == true then
b:Play()
	else
		b:Stop()
	end
	end
