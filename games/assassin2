--AUTOFARM TAB START

local farm = main:Tab({
	Text = "Autofarm",
	Left = "Toggles",
	Right = "Configuration"
})

local toggle = farm:Toggle({
	Text = "Enable Autofarm",
	Side = "L",
	func = function(e)
		getgenv().Autofarm = e
	end
})

_G.RealSpeed = 0
getgenv().spinbot = false
_G.updateSpeed = 0

local toggle = farm:Toggle({
	Text = "Enable Spinbot",
	Side = "L",
	func = function(e)
		getgenv().spinbot = e

        if e == false then
			_G.updateSpeed = _G.updateSpeed
			wait(0.1)
_G.RealSpeed = 0
        end

if e == true then
	_G.RealSpeed = _G.updateSpeed
end

	end
})


farm:Button({
	Text = "Ghost Coin Collect",
	Side = "R",
	func = function()
		loadstring(game:HttpGet('https://raw.githubusercontent.com/Lunar-Eclipse111/Swift.LOL/Scripts/Scripts/assassin/GhostCoinCollect'))()
	end
})


local slider = farm:Slider({
	Text = "SpinBot Speed",
	Side = "R",
	min = 0,
	max = 30,
	prefix = "%",
	func = function(e)
		if getgenv().spinbot == true then
			_G.RealSpeed = e
			end
	
	if getgenv().spinbot == false then
		_G.updateSpeed = e
	end
	end
})
 -- farm tab end


--Crawl ANIMATION START
local a1 = "282574440"
local a2 = Instance.new("Animation")
a2.AnimationId = "rbxassetid://"..a1
local a = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(a2)
a:AdjustSpeed(4) 
-- Crawl ANIMATION END

--lay ANIMATION START

_G.Lay = false
if _G.Lay == true then
	while true do
local b1 = "181526230"
local b2 = Instance.new("Animation")
b2.AnimationId = "rbxassetid://"..b1
local b = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(b2)
b:AdjustSpeed(4) 
b:Play()
	end
end
-- lay ANIMATION END

-- Animation tab 
local ani = main:Tab({
	Text = "Animation",
	Left = "Lay",
	Right = "Crawl"
})



local toggle = ani:Toggle({
	Text = "Lay",
	Side = "L",
	func = function(e)

		if e == true then
			wait()
			local b1 = "181526230"
			local b2 = Instance.new("Animation")
			b2.AnimationId = "rbxassetid://"..b1
			local b = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(b2)
			b:AdjustSpeed(4) 
			b:Play()
		end

		if e == false then
			wait()

			b:Stop()
		end
       

	end
})

-- Animation tab end


-- HIDE GUI SCRIPT

-- SPINBOT SCRIPT
if getgenv().spinbot == true then
	_G.RealSpeed = _G.RealSpeed
	end
	
	if getgenv().spinbot == false then
	_G.RealSpeed = 0
	end
	
	task.spawn(function()
			game:GetService("RunService").Heartbeat:connect(function()
				local hrp = game.Players.LocalPlayer.Character.HumanoidRootPart
	
				-- spining
			hrp.CFrame = hrp.CFrame * CFrame.Angles(0,math.rad(_G.RealSpeed),0)
			wait(0.01)
	
	
			end)
		end)
-- SPINBOT SCRIPT END


