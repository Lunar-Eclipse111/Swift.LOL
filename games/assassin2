while _G.AutoPickup do
for i, v in pairs(game.Workspace.KnifeHost.PowerUps:GetChildren()) do

	if v:IsA("Part") then
		
	 local Player = game.Players.LocalPlayer.Character
   
	 local Target = v.CFrame
   
   
   
	 Player.HumanoidRootPart.CFrame = CFrame.new(Target.Position.X, Target.Position.Y, Target.Position.Z)
   
	end
   
   end
   
   wait(0.25)
   
end
   
   
